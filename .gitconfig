[remote "docker"]
    url = git@github.com:docker/docker-ce.git
    fetch = +refs/heads/*:refs/remotes/docker/*
[remote "github"]
    url = git@github.com:huangyingw/docker-ce.git
    fetch = +refs/heads/*:refs/remotes/github/*
[push]
    remote = github
[gsync]
    remote = docker
    branch = master
[merge]
	allow = true
[branch "dev"]
    remote = github
    merge = refs/heads/dev
[branch "master"]
    remote = docker
    merge = refs/heads/master
[commit]
	message = n
